<?xml version="1.0" encoding="UTF-8"?>
<server>
  <mbean code="org.jboss.mq.server.jmx.Queue"
    name="jboss.esb.#JobPackage#:service=Queue,name=#QueueName#">
    <depends optional-attribute-name="DestinationManager">
      jboss.mq:service=DestinationManager
    </depends>
  </mbean>
  <mbean code="org.jboss.mq.server.jmx.Queue"
    name="jboss.esb.#JobPackage#:service=Queue,name=#QueueName#_ESB">
    <depends optional-attribute-name="DestinationManager">
      jboss.mq:service=DestinationManager
    </depends>
  </mbean>
  <mbean code="org.jboss.mq.server.jmx.Queue"
    name="jboss.esb.#JobPackage#:service=Queue,name=#QueueName#_ESB_reply">
    <depends optional-attribute-name="DestinationManager">
      jboss.mq:service=DestinationManager
    </depends>
  </mbean>
</server>
