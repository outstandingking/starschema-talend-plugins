<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Starschema"
    RELEASE_DATE="20110420A"
    STARTABLE="true"
    HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Business/SAP</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR MULTI_SCHEMA="true" CTYPE="FLOW" MAX_INPUT="0" MIN_OUTPUT="1" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  
  <PARAMETERS>




    <PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="2">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

        <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tSAPConnection"
      NUM_ROW="3"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

	  <PARAMETER
    	NAME="NOTE"
    	FIELD="LABEL"
    	NUM_ROW="1"
    >
    	<DEFAULT>(To use this component, you need first to add the SAP Java Connector (sapjco.jar) in the Modules view)</DEFAULT>
</PARAMETER>

  	<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="4" REPOSITORY_VALUE="SAP" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"/>



    <PARAMETER NAME="SERVERTYPE" FIELD="CLOSED_LIST" NUM_ROW="6" GROUP="CONNECTION" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
      		<ITEMS DEFAULT="APPLICATIONSERVER">
        		<ITEM NAME="APPLICATIONSERVER"   VALUE="ApplicationServer" />
        		<ITEM NAME="MESSAGESERVER"   VALUE="MessageServer" />
        	</ITEMS>
    </PARAMETER>			
  	<PARAMETER NAME="CLIENT" FIELD="TEXT" NUM_ROW="7" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="CLIENT" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<DEFAULT>"000"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="USERID" FIELD="TEXT" NUM_ROW="20" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="USERID" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<DEFAULT>"TALEND"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="PASSWORD" FIELD="TEXT" NUM_ROW="30" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="PASSWORD" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<DEFAULT>"FRANCE"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="LANGUAGE" FIELD="TEXT" NUM_ROW="40" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="LANGUAGE" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<DEFAULT>"EN"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="HOSTNAME" FIELD="TEXT" NUM_ROW="50" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="HOSTNAME" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<DEFAULT>"HOST"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="SYSTEMNUMBER" FIELD="TEXT" NUM_ROW="60" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="SYSTEMNUMBER" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SERVERTYPE=='APPLICATIONSERVER')">
		<DEFAULT>"00"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="SYSTEMID" FIELD="TEXT" NUM_ROW="60" GROUP="CONNECTION" REQUIRED="true" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SERVERTYPE=='MESSAGESERVER')">
		<DEFAULT>"PRD"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="GROUPNAME" FIELD="TEXT" NUM_ROW="70" GROUP="CONNECTION" REQUIRED="true" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SERVERTYPE=='MESSAGESERVER')">
		<DEFAULT>"tGroup"</DEFAULT>
	</PARAMETER>


	<PARAMETER NAME="SAP_TABLE_NAME" FIELD="TEXT" NUM_ROW="75" REQUIRED="true" REPOSITORY_VALUE="SAP_TABLE_NAME">
		<DEFAULT>"INSERT_TABLE_NAME"</DEFAULT>
	</PARAMETER>
		
<PARAMETER
  NAME="SAP_FUNCTION"
  FIELD="CLOSED_LIST"
  NUM_ROW="77"
  REQUIRED="true"
  REPOSITORY_VALUE="SAP_FUNCTION"
  >
  <ITEMS DEFAULT="Z_PLSAP_RFC">
    <ITEM NAME="Z_PLSAP_RFC" VALUE="Z_PLSAP_RFC" />
    <ITEM NAME="Z_PLSAP_ENHANCED"   VALUE="Z_PLSAP_ENHANCED" />
    <ITEM NAME="Z_PLSAP_FTP" VALUE=" Z_PLSAP_FTP" />
  </ITEMS>
</PARAMETER>


	
  	<PARAMETER NAME="SAP_OUTPUT_DIRECTORY" FIELD="TEXT" NUM_ROW="80" REQUIRED="true" REPOSITORY_VALUE="SAP_OUTPUT_DIRECTORY">
		<DEFAULT>"/var/tmp/"</DEFAULT>
	</PARAMETER>


	<PARAMETER NAME="SAP_TABLE_COLUMNS" FIELD="TABLE" NUM_ROW="90"	NB_LINES="1">
		<ITEMS>
			<ITEM NAME="FIELD_NAME" FIELD="FIELD_NAME" />	
			<!--
			<ITEM NAME="SAP_ITERATE_OUT_TYPE" FIELD="CLOSED_LIST">
				<ITEMS DEFAULT="OUTPUT_SINGLE">
					<ITEM NAME="TABLE_FIELD_NAME" VALUE="table_field_name" />
					<ITEM NAME="OUTPUT_SINGLE" VALUE="output_single" />
					<ITEM NAME="OUTPUT_STRUCTURE" VALUE="output_structure" />
					<ITEM NAME="OUTPUT_TABLE" VALUE="output_table" />
				</ITEMS>
			</ITEM>
			<ITEM NAME="SAP_TABLE_NAME" FIELD="String" />						
			<ITEM NAME="MAPPING" FIELD="SCHEMA_XPATH_QUERYS" />
			-->
		</ITEMS>
	</PARAMETER>

	<PARAMETER
  		NAME="WHERE_CONDITION"
  		FIELD="MEMO"
  		REQUIRED="true"
  		NB_LINES="10"
  		NUM_ROW="100"
  	>
  		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
  		NAME="COMPRESSION"
  		FIELD="CHECK"
  		REQUIRED="false"
		NUM_ROW="110"
		SHOW_IF="(SAP_FUNCTION == 'Z_PLSAP_ENHANCED')"
  	>
  		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	

  </PARAMETERS>
  <ADVANCED_PARAMETERS>
  	<PARAMETER
      NAME="CLOSE"
      FIELD="CHECK"
      NUM_ROW="10"
    >
    <DEFAULT>true</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>
  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="sapjco-ntintel-2.1.8" MODULE="sapjco.jar" REQUIRED="true" />       
     </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS/>
  
</COMPONENT>
